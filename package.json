{
  "name": "tailwindcss-fluid-responsive-design",
  "version": "0.0.1",
  "author": "penrodlol",
  "private": true,
  "scripts": {
    "watch:tw": "npx tailwindcss -i ./src/styles.css -o ./dist/styles.css --watch",
    "watch:html": "live-server --entry-file=src/index.html",
    "dev": "npm-run-all --parallel watch:tw watch:html"
  },
  "devDependencies": {
    "live-server": "^1.2.2",
    "npm-run-all": "^4.1.5",
    "tailwindcss": "^3.1.8"
  }
}
